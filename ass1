
#include <stdio.h>

int main() {
    int number, i, isPrime = 1;

    printf("Enter a positive integer: ");
    scanf("%d", &number);

    if (number <= 1) {
        printf("%d is not a prime number.\n", number);
        return 0;
    }

    for (i = 2; i < number; i++) {
        if (number % i == 0) {
            printf("no. is not prime.\n");
            return 0;
        }
    }
  printf("no.. is notprime");
  return 0;
}
